func fact(n) {
    if (n == 1) {
        1;
    } else {
        n * fact(n - 1);
    }
}
print("fact(5) is ");
println(fact(5));

func fib(n) {
    if (n == 0) {
        0;
    } else if (n == 1) {
        1;
    } else {
        n1 = fib(n-1);
        n2 = fib(n-2);
        n1 + n2;
    }
}
print("fib(6) is ");
println(fib(6));
