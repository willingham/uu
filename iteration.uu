func iterate(x) {
    for(i = 0; i < x; i = i + 1) {
        print("i is ");
        println(i);
    }

    i = 0;
    while (i < x) {
        print("i is ");
        println(i);
        i = i + 1;
    }

    i = 0;
    while (i < 5) {
        println(i);
        if (i == 3) {
            break;
        }
        i = i + 1;
}
}
iterate(5);
